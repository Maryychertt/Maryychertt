# This is a basic counter program in Python

def counter(start, end):
    """Counts from start to end and prints each number."""
    for i in range(start, end + 1):
        print(i)

# Example usage
if __name__ == "__main__":
    start = 1
    end = 10
    print(f"Counting from {start} to {end}:")
    counter(start, end)
